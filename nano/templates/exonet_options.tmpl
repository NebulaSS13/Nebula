{{if data.error}}
	<h2>An error has occured:</h2>
	Additional information: {{:data.error}}<br>
	<i>Please try again. If the problem persists contact your system administrator for assistance.</i>
	<div class="item">
		{{:helper.link('Refresh', null, { "refresh" : 1 })}}
	</div>
{{else}}
	<i>Welcome to the EXONET network configuration utility. Please consult your system administrator if you have any questions about your device.</i><hr>
	<h2>Power Supply</h2>
	<div class="itemLabel">
		Power Usage: 
	</div>
	<div class="itemContent">
		{{:data.power_usage}}kW
	</div>
	<h2>Network Configuration</h2>
	<div class="itemLabel">
		Network Status:
	</div>
	<div class="itemContent">
		{{:data.status}}
	</div>
	<div class="itemLabel">
		Network ENNID:
	</div>
	<div class="itemContent">
		{{:helper.link(data.ennid, null, { "change_ennid" : 1 })}}
	</div>
	<div class="itemLabel">
		Network KEY:
	</div>
	<div class="itemContent">
		{{:helper.link(data.key, null, { "change_key" : 1 })}}
	</div>
	<div class="itemLabel">
		Network TAG:
	</div>
	<div class="itemContent">
		{{:helper.link(data.net_tag, null, { "change_net_tag" : 1 })}}
	</div>
	<div class="item">
		{{:helper.link("RETURN", null, { "quit_options" : 1 })}}
	</div>
	<hr>
	<i>EXONET Firmware v110.04.4h Copyright EXONETWORKS INC</i>
{{/if}}